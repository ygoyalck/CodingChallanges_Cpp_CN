#include<iostream>
using namespace std;

int main() {
	int n;
  cin>>n;
  if(n==1)
    cout<<1<<endl;
  else if(n==2)
   { cout<<1<<endl;
  cout<<11<<endl;}
  else{
    cout<<1<<endl;
  cout<<11<<endl;
    int k=1;
    for(int i=2;i<=n-1;i++)
    {
      cout<<i;
      for(int j=0;j<k;j++)
        cout<<0;
      cout<<i;
      k+=1;
      cout<<endl;
    }
  }
	
}
